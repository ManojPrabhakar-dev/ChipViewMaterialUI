<controls:MetroWindow x:Class="ChipViewConfig.MainWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:ChipViewConfig"
	xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
	xmlns:uc="clr-namespace:ChipViewConfig"
	xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
	xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
	xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
	xmlns:dockablz="clr-namespace:Dragablz.Dockablz;assembly=Dragablz"                  
	WindowTransitionsEnabled="False"
	Background="{StaticResource App_Background}"
	GlowBrush="{DynamicResource AccentColorBrush}"					  
	FontFamily="{StaticResource DefaultFont}"	
	TitleForeground="Black"	
	Name="LayoutWin"
	Title="ChipView - Experimental" Height="770" Width="1200">

	<!--BorderThickness="2,5,2,2"-->
	<!--Background="{DynamicResource MaterialDesignPaper}"-->
	<!--TextElement.Foreground="{DynamicResource MaterialDesignBody}"-->
	<Window.Resources>
		<ContextMenu x:Key="InpMuxMenu">
			<MenuItem Header="Enable CH2" Click="MenuItem_Click" />
		</ContextMenu>
		<LinearGradientBrush x:Key="ProgressBrush" EndPoint="0.001,0.5" StartPoint="1.002,0.5">
			<GradientStop Color="{DynamicResource Primary700}" Offset="0"/>
			<GradientStop Color="{DynamicResource Primary100}" Offset="1"/>
		</LinearGradientBrush>
		<LinearGradientBrush x:Key="SettingsBrush" StartPoint="0,0" EndPoint="1,1">
			<GradientStop Color="{DynamicResource Primary700}" Offset="0"/>
			<GradientStop Color="#0000FFFF" Offset="1"/>
		</LinearGradientBrush>
		<LinearGradientBrush xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="linearGradient8961" MappingMode="RelativeToBoundingBox" StartPoint="0,0" EndPoint="1,1">
			<LinearGradientBrush.GradientStops>
				<GradientStopCollection>
					<GradientStop Color="#FF00D4AA" Offset="0"/>
					<GradientStop Color="#0000D4AA" Offset="1"/>
				</GradientStopCollection>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
		<LinearGradientBrush xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="disablegradient" MappingMode="RelativeToBoundingBox" StartPoint="0,0" EndPoint="1,1">
			<LinearGradientBrush.GradientStops>
				<GradientStopCollection>
					<GradientStop Color="Gray"  Offset="0"/>
					<GradientStop Color="White"  Offset="1"/>
				</GradientStopCollection>
			</LinearGradientBrush.GradientStops>
		</LinearGradientBrush>
	</Window.Resources>

	<Grid Name="MainGrid">
		<Grid.RowDefinitions>            
			<RowDefinition Height="55"/>
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions >
			<ColumnDefinition Width="890"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>


		<materialDesign:Card Margin="5" Grid.Row="0" Grid.Column="1" Grid.RowSpan="2" Background="{StaticResource SettingsBackground}" materialDesign:ShadowAssist.ShadowDepth="Depth3">
				<ScrollViewer Name="SettingsScroll" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" >
					<Grid Name="SettingsWin" >
						<Grid.RowDefinitions>
							<RowDefinition Height="1"/>
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
						</Grid.RowDefinitions>

					<Expander Name="RadGlobalSettings" ExpandDirection="Down" Header="Global Settings" Visibility="Visible" 
								 FontSize="14" Grid.Row="1" FontWeight="Bold" >
							<Grid Name="GlobalSettings"  Margin="0, 0, 0, 20">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="0.5*"/>
									<ColumnDefinition Width="0.5*"/>
								</Grid.ColumnDefinitions>
							</Grid>
						</Expander>

					<Border Grid.Row="2" Background="{DynamicResource MaterialDesignDivider}"
						  Height="1"
						  HorizontalAlignment="Stretch"
						  SnapsToDevicePixels="True" />

					<Expander Name="RadExpSettings"  Grid.Row="3" ExpandDirection="Down" Header="Slot Configuration" 
								 FontSize="14" FontWeight="Bold"
								 Visibility="Visible" >
							<Grid Name="SlotGlobalSettings" Margin="0, 0, 0, 20" Height="Auto" Width="Auto">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="0.5*"/>
									<ColumnDefinition Width="0.5*"/>
								</Grid.ColumnDefinitions>
							</Grid>
						</Expander>
					<Border Grid.Row="4" Background="{DynamicResource MaterialDesignDivider}"
					  Height="1"
					  HorizontalAlignment="Stretch"
					  SnapsToDevicePixels="True" />

					<Expander Name="RadExpTimingSettings" Grid.Row="5" ExpandDirection="Down" Header="Timing Configuration"
								 FontSize="14" FontWeight="Bold"
								 Visibility="Visible"  >
							<Grid Name="TimingSettings" Margin="0, 0, 0, 20" Height="Auto" Width="Auto">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="0.5*"/>
									<ColumnDefinition Width="0.5*"/>
								</Grid.ColumnDefinitions>
							</Grid>
						</Expander>

					</Grid>
				</ScrollViewer>
			</materialDesign:Card>
	
		<!--<materialDesign:ColorZone Grid.ColumnSpan="2" Padding="16" materialDesign:ShadowAssist.ShadowDepth="Depth2"
											Mode="PrimaryMid" >
			<DockPanel>
				--><!--<ComboBox x:Name="SlotSel" Foreground="White" Background="LightGray" HorizontalAlignment="Left"
						  Margin="20,5,0,5" VerticalAlignment="Center" FontSize="16" FontWeight="ExtraBold" FontStretch="ExtraExpanded"
						  Height ="30" Width="100" Loaded="SlotSelComboBox_Loaded" SelectionChanged="SlotSelComboBox_SelectionChanged" />--><!--

				<TextBlock HorizontalAlignment="Center" Foreground="WhiteSmoke"  FontSize="22">Chip Design View - Experimental</TextBlock>

				--><!--<Button Content="Apply" Click="OnApplyConfigClicked" Width="70" Height="30" FontSize="14" FontWeight="DemiBold"
						HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,10,0"/>--><!--
			</DockPanel>
		</materialDesign:ColorZone>-->
		
		<materialDesign:Card Grid.Row="0" Margin="5"  Background="{DynamicResource MaterialDesignBackground}" materialDesign:ShadowAssist.ShadowDepth="Depth1">
			<StackPanel Height="48" VerticalAlignment="Top" >
				<Grid Name="TopControls" Grid.Row="0">
					<ComboBox x:Name="SlotSel" HorizontalAlignment="Left" Margin="20,5,0,5" VerticalAlignment="Top" FontSize="16" FontWeight="ExtraBold" FontStretch="ExtraExpanded"
							  Height ="30" Width="100" Loaded="SlotSelComboBox_Loaded" SelectionChanged="SlotSelComboBox_SelectionChanged" />
					<Button Content="Apply" Click="OnApplyConfigClicked" Foreground="Black" Width="70" Height="30" FontSize="14" FontWeight="DemiBold" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,5,0"/>
				</Grid>
			</StackPanel>
		</materialDesign:Card>

		<!--<TabControl Grid.Row="2" SelectedIndex="1">-->

		<dockablz:Layout Grid.Row="1">
			<dragablz:TabablzControl BorderThickness="0"
								 Margin="5,-1,5,2" SelectedIndex="0">
				
				<dragablz:TabablzControl.InterTabController>
					<dragablz:InterTabController />
				</dragablz:TabablzControl.InterTabController>

